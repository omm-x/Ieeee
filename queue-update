#include <stdio.h>
#define MAX 5

int queue[MAX];
int front = -1;
int rear = -1;

int isFull() {
    return rear == MAX - 1;
}

int isEmpty() {
    return front == -1 || front > rear;
}

void enqueue(int x) {
    if (isFull()) {
        printf("Overflow\n");
        return;
    }
    if (front == -1) front = 0;
    rear++;
    queue[rear] = x;
    printf("%d inserted\n", x);
}

int dequeue() {
    if (isEmpty()) {
        printf("Underflow\n");
        return -1;
    }
    int val = queue[front];
    front++;
    printf("%d removed\n", val);
    return val;
}

int main() {
    int choice, value;
    while (1){
        printf("1. Enqueue\n");
        printf("2. Dequeue\n");
        printf("3. Exit\n");
        printf("Enter your choice: ");
        scanf("%d", &choice);

        switch (choice) {
            case 1:
                printf("Enter value to enqueue: ");
                scanf("%d", &value);
                enqueue(value);
                break;
            case 2:
                value = dequeue();
                if (value != -1) {
                    printf("Dequeued is %d\n", value);
                }
                break;
            case 3:
                printf("Exiting...\n");
                return 0;
            default:
                printf("Invalid choice.\n");
        }
    }
}
