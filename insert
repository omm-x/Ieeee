#include<stdio.h>
void insertionSort(int a[],int n){
    int i,j,temp;
    for(i=1;i<n;i++){
        temp=a[i];
        j=i-1;

        while(j>=0 && a[j]>temp){
            a[j+1]=a[j];
            j--;
        }
        a[j+1]=temp;
    }
}
int main(){
    int n,i;
    printf("enter number of elements= ");
    scanf("%d",&n);
    int a[n];
    printf("ENter %d elements= \n",n);
    for(i=0;i<n;i++){
        scanf("%d",&a[i]);
    }
    insertionSort(a,n);
    printf("sorted elements= ");
    for(i=0;i<n;i++){
        printf("%d\t",a[i]);
    }
    return 0;
}
